do
        bool Zet_die_HP_maar_op_0_jongens(Act,Target)
		if math.random(1,12 /__skill)==1
	                Target.Char.Points("HP").Have = 0
        	        return true
		else
			if not prefixed(Target.Tag,"FOE_")
				Target.Char.Stat("Experience").Have = - math.abs(Target.Char.Stat("Experience").Have)
			end
			return false
		end
        end

        return Zet_die_HP_maar_op_0_jongens
end
